<h2> Destinations <h2>
<% @destinations.each do |destination| %>
    <ul>
        <h5><li><%=destination.location %>
        <%= link_to "Add Destination", destination_path(destination, trip_id:@trip.id,user_id:@user.id)%>
        <%= image_tag(destination.image, :size => "260x180") %></li></h5>
    </ul>
<%end%>


         
         
       

         